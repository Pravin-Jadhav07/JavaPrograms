
class Animal{
    protected void eat(){
        System.out.println("eating....");
    }
}

class Dog extends Animal{
    public void voice(){
        System.out.println("bow$ bow$$..");
    }
}

public class SingleInheritance {
    public static void main(String[] args) {
        Dog d1 = new Dog();
        d1.eat();
        d1.voice();
    }
}
