import java.util.Scanner;

public class Demo {
    public static void main(String[] args) 
    {
        Scanner sc = new Scanner(System.in);

        System.out.println("enter a character ");
        char ch = sc.next().charAt(0);

        int ascii = ch;
        System.out.println("accii = " + ascii );

        
        sc.close();
    }
}
