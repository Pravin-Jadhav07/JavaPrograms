import java.util.Scanner;

public class Demo {
    public static void main(String[] args) 
    {
        Scanner sc = new Scanner(System.in);

        System.out.println("enter a number ");
        int num = sc.nextInt();

        int result = num & 1;

        if( result < 1 ){
            System.out.println("Number is Even.");
        }
        else{
            System.out.println("Number is Odd");
        }

        
        sc.close();
    }
}
